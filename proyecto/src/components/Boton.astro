---
export interface Props {
  href: string; 
}
const { href } = Astro.props;
---
<a type="button" href={href} class="border-yellow-400 dark:border-yellow-300 hover:bg-yellow-500 dark:hover:bg-yellow-400 mb-2 px-5 py-2.5 border rounded-lg focus:ring-4 focus:ring-yellow-300 dark:focus:ring-yellow-900 font-medium text-center text-sm text-yellow-400 hover:text-white focus:outline-none dark:hover:text-white dark:text-yellow-300 me-2">
    <slot />
</a>